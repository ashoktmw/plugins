      "name": "httpProtocol",
      "type": "STRING",
      "filterable": true,
      "filterType": "ENUM",
      "filterEnumValues": [
        "http1.1",
        "https1.1",
        "http2",
        "http3"
      ],
      "authorizable": false
    }
