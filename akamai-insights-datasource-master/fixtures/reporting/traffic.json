      "edgeBytesSum"
    ],
    "defaultDimensions": [
      "hostname",
      "responseCode"
    ],
    "defaultSortBys": [
      {
        "name": "hostname",
        "sortOrder": "ASCENDING"
      },
      {
        "name": "edgeHitsSum",
        "sortOrder": "DESCENDING"
      }
