        "secure"
      ],
      "authorizable": false
    },
    {
      "name": "cacheability",
      "type": "STRING",
      "filterable": true,
      "filterType": "ENUM",
      "filterEnumValues": [
        "cacheable",
        "non_cacheable"
      ],
      "authorizable": false
    },
    {
      "name": "ipVersion",
      "type": "STRING",
      "filterable": true,
      "filterType": "ENUM",
      "filterEnumValues": [
